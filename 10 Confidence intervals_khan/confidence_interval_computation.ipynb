   "source": [
    "import scipy.stats as stats\n",
    "import numpy as np\n",
    "\n",
    "a = stats.t\n",
    "n = 14\n",
    "df = n-1\n",
    "t_star = a.ppf(0.975, df)\n",
    "\n",
    "x_bar = 700\n",
    "s = 50\n",
    "ci_95 = np.array([x_bar-t_star*s/np.sqrt(n), x_bar+t_star*s/np.sqrt(n)])\n",
    "print(ci_95)\n",
    "print(t_star*s/np.sqrt(n))"
   ]
